<div class="row">
  {{#each comicBooks}}
            <div class="col">
                <div class="card">
                    <img src="{{comicCover}}" height="500px" class="card-img-top" alt="...">
                    <div class="card-body">
                      <h5 class="card-title">{{title}}</h5>
                      <p class="card-text">{{universe}}</p>
                      <p class="card-text">{{year}}</p>
                      
                      <a href="/InventoryEntry" class="btn btn-primary">Edit Comic Book</a><a href="#" class="float-end btn btn-danger" onclick="deleteComicByNum">Delete Comic Book</a>
                    </div>
                  </div>
            </div>
          {{/each}}

          <nav aria-label="Pages">
            <ul class="pagination justify-content-center">
              <li class="page-item">
                <a class="page-link" href="#" tabindex="-1">Previous</a>
              </li>
              <li class="page-item active"><a class="page-link" href="#">1</a></li>
              <li class="page-item">
                <a class="page-link" href="#">2 </a>
              </li>
              <li class="page-item"><a class="page-link" href="#">3</a></li>
              <li class="page-item">
                <a class="page-link" href="#">Next</a>
              </li>
            </ul>
          </nav>
    </div>